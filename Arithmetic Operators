#include <stdio.h>

int main() 
{
    int a, b, sum, diff, product;
    float qoutient;
    printf("Enter a: ");
    scanf("%d", &a);
    printf("Enter b: ");
    scanf("%d", &b);
    
    sum = a+b;
    printf("a+b = %d\n",sum);
    
    diff = a-b;
    printf("a-b = %d\n",diff);
    
    product = a*b;
    printf("a*b = %d\n",product);
    
    qoutient = a/b;
    printf("a/b = %.f\n",qoutient);
    
    return 0;
    
}
